{
  "Logging": {
    "IncludeScopes": true,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.Hosting.Lifetime": "Information",
      "OrchardCore": "Debug",
      "OrchardCore.Flows": "Trace",
      "OrchardCore.DisplayManagement": "Trace",
      "OrchardCore.ContentManagement": "Debug"
    }
  },
  "AllowedHosts": "*",
  "OrchardCore": {
    "OrchardCore_Tenants": {
      "TenantRemovalAllowed": true
    }
  }
}
